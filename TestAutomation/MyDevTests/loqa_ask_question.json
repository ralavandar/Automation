{
  "type": "script",
  "seleniumVersion": "2",
  "formatVersion": 2,
  "steps": [
    {
      "type": "get",
      "url": "https://www.staging.lendingtree.com/answers"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "Title"
      },
      "text": "this is a title hello sean"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "Detail"
      },
      "text": "this is a question"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "css selector",
        "value": ".form-submit button"
      }
    },
    {
      "type": "waitForCurrentUrl",
      "url": "https://www.staging.lendingtree.com/question/ask-details"
    }
  ],
  "data": {
    "configs": {},
    "source": "none"
  },
  "inputs": [],
  "timeoutSeconds": 60
}