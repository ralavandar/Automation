  <DataSets>
    <DataSet Name="SystemObjects">
      <Query>
        <DataSourceName>LXDB</DataSourceName>
        <CommandText>SELECT t.name AS table_name,
SCHEMA_NAME(schema_id) AS schema_name,
c.name AS column_name
FROM sys.tables AS t with (nolock)
INNER JOIN sys.columns c  with (nolock) ON t.OBJECT_ID = c.OBJECT_ID
WHERE c.name LIKE '%' + @tableNameSegment + '%'
ORDER BY schema_name, table_name</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
  </DataSets>
  <ReportParameters>
    <ReportParameter Name="tableNameSegment">
      <DataType>String</DataType>
      <Prompt>Part of Column Name to Search by</Prompt>
    </ReportParameter>
